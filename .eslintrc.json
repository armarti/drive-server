{
  "extends": "airbnb-base",
  "rules": {
    "semi": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "prefer-destructuring": 0,
    "import/no-dynamic-require": 0,
    "prefer-arrow-callback": 0,
    "no-unused-vars": 0,
    "arrow-body-style": 0,
    "prefer-template": 0,
    "eqeqeq": 1,
    "max-len": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "prefer-promise-reject-errors": 0,
    "object-shorthand": "off",
    "consistent-return": "off",
    "no-unused-expressions": "off"
  }
}
